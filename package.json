{
  "name": "decentralized-event-network",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:Continuities/decentralized-event-network.git",
  "author": "Michael Townsend <michael@doublespeakgames.com>",
  "license": "MIT",
  "scripts": {
    "dev:install": "./script/install-dev.sh",
    "dev:up": "cd docker && docker-compose up -d",
    "dev:down": "cd docker && docker-compose down && docker-compose rm -vf",
    "dev:logs": "cd docker && docker-compose logs -f",
    "dev:ps": "cd docker && docker-compose ps",
    "dev:restart": "cd docker && docker-compose restart",
    "dev:uninstall": "cd docker && docker-compose down --rmi all",
    "dev:connect:api": "docker exec -it docker_api_1 /bin/sh",
    "dev:connect:app": "docker exec -it docker_app_1 /bin/sh",
    "dev:connect:db": "docker exec -ti den-db mongo",
    "dev:connect:server": "docker exec -ti docker_server_1 /bin/sh"
  },
  "dependencies": {}
}
